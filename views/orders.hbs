<div id="tupleList">
    <h2>Orders</h2>

    <table>
        <thead>
            <tr>
                {{#each data.[0]}}
                <th>
                    {{@key}}
                </th>
                {{/each}}
            </tr>
        </thead>

        <tbody>
            {{#each data}}
            <tr>
                <td>{{this.orderID}}</td>
                {{#if this.customer}}
                    <td>{{this.customer}}</td>
                {{else}}
                    <td><em>NULL</em></td>
                {{/if}}
                {{#if this.game}}
                    <td>{{this.game}}</td>
                {{else}}
                    <td><em>NULL</em></td>
                {{/if}}
                <td>{{this.orderDate}}</td>
                <td>{{this.pricePaid}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
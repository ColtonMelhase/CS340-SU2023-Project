<div id="tupleList">
    <h2>Games</h2>

    <table>
        <thead>
            <tr>
                {{#each data.[0]}}
                <th>
                    {{@key}}
                </th>
                {{/each}}
            </tr>
        </thead>

        <tbody>
            {{#each data}}
            <tr>
                <td>{{this.gameID}}</td>
                <td>{{this.studio}}</td>
                <td>{{this.title}}</td>
                <td>{{this.publishDate}}</td>
                <td>{{this.price}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>